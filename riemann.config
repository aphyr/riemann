; vim: filetype=clojure

(logging/init :file "riemann.log")

(tcp-server)
(udp-server)
(ws-server)

(periodically-expire 10)

(let [index (default :ttl 300 (update-index (index)))]
  (streams
    index
    (tagged-any "tag"
                (partial prn "HEYO"))
    (where (tagged "foo")
           (partial prn "foo")
           (else
             (partial prn "not foo"))))
)
